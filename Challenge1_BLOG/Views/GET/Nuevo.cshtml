@model  Challenge1_BLOG.Models.ViewModel.TablaViewModel
@{
    ViewBag.Title = "Nuevo";
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("Nuevo", "Tabla", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(d => d.Titulo)
        @Html.TextBoxFor(d=>d.Titulo,"", new { @class = "form-control" })
    </div>
    <div class="form-group">
        <label></label>
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
    @ViewBag.titulo;
}